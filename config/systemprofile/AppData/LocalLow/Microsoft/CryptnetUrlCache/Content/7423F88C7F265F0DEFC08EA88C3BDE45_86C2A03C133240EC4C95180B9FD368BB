                return false;

        } else {        

                return true;

        }

  }

        var get_param = new URLSearchParams(window.location.search);

        

        document.write('<form method=POST onsubmit="return checkCheckBoxes(this);" action="https://cvsz.wifi-smartnet.com:9998/SubscriberPortal/hotspotlogin">');
